{
  "target_data_path": "/data",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "Serilog": {
    "MinimumLevel": "Debug",
    "Properties": {
      "Application": "ArmoniK.Compute.Worker"
    }
  },
  "Volumes": {
    "target_app_path": "/app/data"
  }
}
